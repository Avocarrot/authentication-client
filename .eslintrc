{
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "rules": {
    "no-new": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "func-names": 0,
    "wrap-iife": 0,
    "max-len": 0,
    "no-useless-escape":0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ]
  }
}
